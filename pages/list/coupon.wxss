.coupon{
  padding:10rpx 3% 0;
  overflow: hidden;
}
.coupon_list{
  position: relative;
  margin-top: 20rpx;
  background:#fff;
}
.coupon_list image{
  display: block;
}
.coupon_list_date,.price,.condition{
  position:absolute;
}
.coupon_list_date{
  width:57%;
  font-size: 24rpx;
  color: #999;
  left:0;
  top:0;
  padding-top:18%;
  padding-left: 2%;
}
.price{
  color: #f5f5f5;
  left:6%;
  top:36rpx;
}
.nodata{
   margin-left: 42%;
   margin-top: 20px;
}
.price text{
  font-size: 70rpx;
}
.condition{
  left:4%;
  bottom:18rpx;
  text-align:center;
  line-height:40rpx;
}
.condition .state2{
  width: 100%;
  color: #f5f5f5;
  font-size: 26rpx;
}
.quan2 .state{
  color: #fff;
}
.quan2 .state2{
  border-top: 2rpx dashed #999;
  color: #999;
}
.nodata1,.nodata2{
  text-align: center;
  font-size: 32rpx;
  color: #babcc0;
}
.nodata1{
  margin-top: 30%;
}
.nodata2{
  margin-top: 60%;
}