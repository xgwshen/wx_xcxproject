view{
  font-size: 30rpx;
}
.top_nav{
  padding: 0 5%;
  width: 90%;
  line-height: 90rpx;
  overflow: hidden;
  background: #fff;
  text-align: center;
  z-index: 2;
  position: fixed;
  top: 24rpx;
  color: #1c2951;
}
.top_nav view{
  width:33.33%;
  display: inline-block;
}
.top_nav .top_navbar_check{
  color: #d70a1d;
}
.top_nav .top_navbar_check text{
  display: inline-block;
  line-height: 90rpx;
  border-bottom: 2rpx solid #d70a1d;
}
.coupon{
  padding:114rpx 3% 0;
  overflow: hidden;
}
.coupon_list{
  position: relative;
  margin-top: 30rpx;
  background:#fff;
}
.coupon_list image{
  display: block;
}
.coupon_list_date,.price,.condition{
  position:absolute;
}
.coupon_list_date{
  width:57%;
  font-size: 24rpx;
  color: #999;
  left:0;
  top:0;
  padding-top:18%;
  padding-left: 2%;
}
.price{
  color: #f5f5f5;
  left:6%;
  top:36rpx;
}
.nodata1,.nodata2{
  text-align: center;
  font-size: 32rpx;
  color: #babcc0;
}
.nodata1{
  margin-top: 30%;
}
.nodata2{
  margin-top: 60%;
}
.price text{
  font-size: 70rpx;
}
.condition{
  left:4%;
  bottom:18rpx;
  text-align:center;
  line-height:40rpx;
}
.condition .state{
  color: #f5f5f5;
  font-size: 55rpx;
}
.condition .state2{
  width: 100%;
  color: #f5f5f5;
  font-size: 26rpx;
}
.condition .state2.state3{
  position: relative;
  left: 10%;
}
.quan2 .state{
  color: #fff;
}
.quan2 .state2{
  border-top: 2rpx dashed #999;
  color: #999;
}